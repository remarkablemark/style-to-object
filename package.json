{
  "name": "style-to-object",
  "version": "0.3.0",
  "description": "Converts inline style to object.",
  "author": "Mark <mark@remarkablemark.org>",
  "main": "index.js",
  "types": "index.d.ts",
  "module": "index.mjs",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:min": "NODE_ENV=production rollup --config --file dist/style-to-object.min.js --sourcemap",
    "build:unmin": "NODE_ENV=development rollup --config --file dist/style-to-object.js",
    "clean": "rm -rf dist",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "lint:dts": "dtslint .",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable && run-s lint lint:dts test clean build",
    "release": "standard-version --no-verify",
    "test": "mocha",
    "test:coverage": "nyc npm test",
    "test:watch": "mocha --watch",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/remarkablemark/style-to-object"
  },
  "bugs": {
    "url": "https://github.com/remarkablemark/style-to-object/issues"
  },
  "keywords": [
    "style-to-object",
    "inline",
    "style",
    "parser",
    "css",
    "object",
    "pojo"
  ],
  "dependencies": {
    "inline-style-parser": "0.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "dtslint": "^4.1.6",
    "eslint": "^8.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "mocha": "^9.1.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "pinst": "^2.1.6",
    "prettier": "^2.4.1",
    "rollup": "^2.58.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9",
    "typescript": "^4.4.4"
  },
  "files": [
    "/dist",
    "index.d.ts",
    "index.mjs"
  ],
  "license": "MIT"
}
